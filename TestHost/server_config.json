{
  "serverName": "My Game Server",
  "serverPort": 9050,
  "serverConnectionKey": "SecretKey",
  "serverMaxPlayers": 10,
  "serverGameMode": 0,
  "serverAllowCheats": false,
  "serverAdminList": ["player_id1", "player_id2"],
  "serverBanList": ["player_id1_banned", "player_id2_banned"],

  "networkTickRate": 20,
