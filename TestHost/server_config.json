  "networkEnableHeartbeat": true,
  "networkHeartbeatInterval": 5000,

  "loggingEnableConsole": true,
  "loggingEnableTick": false,
  "loggingPlayerEvents": true,
  "loggingNetworkEvents": true,
  "loggingEnableFile": false,
  "loggingFilePath": "server.log",
  "loggingEnableErrors": true,
  "loggingEnablePerformance": false
}
